<idea-plugin>
    <id>com.your.company.unique.plugin.id</id>
    <name>Code Helper</name>
    <version>1.0</version>
    <vendor email="jianyongqiao@qq.com" url="http://blog.qjyoung.top">qjyoung</vendor>
    
    <description><![CDATA[
      A simple code-helper plugin.<br>
      <a href="http://blog.qjyoung.top">查看我的博客</a>
    ]]></description>
    
    <change-notes><![CDATA[
      version 1.0.<br>
    ]]>
    </change-notes>
    
    <!--<idea-version since-build="173.0"/>-->
    <idea-version since-build="131"/>
    
    <extensions defaultExtensionNs="com.intellij">
        <projectService serviceImplementation="com.siyueren.codehelper.service.CodeService"/>
        <intentionAction>
            <className>com.siyueren.codehelper.intention.GenerateSetterCallsIntention</className>
        </intentionAction>
        
        <codeInsight.template.postfixTemplateProvider language="JAVA"
                                                      implementationClass="com.siyueren.codehelper.intention.postfix.CodeHelperPostfixTemplateProvider"/>
    </extensions>
    
    <actions>
        <!-- Add your actions here -->
        <!--<action id="CodeHelper.MakeNotes" class="com.siyueren.codehelper.MakeNotes" text="Make Notes" description="Make notes for selection">-->
        <!--<add-to-group group-id="EditorPopupMenu" anchor="last"/>-->
        <!--<keyboard-shortcut keymap="$default" first-keystroke="shift meta M"/>-->
        <!--</action>-->s
    </actions>

</idea-plugin>